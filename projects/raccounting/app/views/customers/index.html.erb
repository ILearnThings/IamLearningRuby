<h1>Customer#index</h1>
<div> <%= link_to 'New Customer', new_customer_path %> </div>
<table>

<tr>
<th>Company Title</th>
<th>Short Name</th>
</tr>

<% @customers.each do |customer| %>
    <tr>
<td><%= customer.company_title %></td>
<td><%= customer.short_name %></td>
<td><%= link_to 'Show', customer_path(customer) %></td>
<td><%= link_to 'Edit', edit_customer_path(customer) %></td>
</tr>
<% end %>
</table>